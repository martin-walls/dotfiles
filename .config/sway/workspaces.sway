set $ws1   1
set $ws2   2
set $ws3   3
set $ws4   4
set $ws5   5
set $ws6   6
set $ws7   7
set $ws8   8
set $ws9   9
set $ws0   10
set $wsF1  11:ï§†
set $wsF2  12:K
set $wsF3  13
set $wsF4  14
set $wsF5  15
set $wsF6  16
set $wsF7  17
set $wsF8  18
